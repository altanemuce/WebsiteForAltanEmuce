@model WebsiteForAltanEmuce.Models.EntityFramework.Portfolio



@{
    ViewBag.Title = "GetPortfolio";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<form method="post">
    @Html.Label("Portfolioid")
    @Html.TextBoxFor(x => x.Porfolioid, new {@class="form-control form-control-user" })
    <br />
    @Html.Label("Portfolioname", new { @class = "form-control" })
    @Html.TextBoxFor(x => x.Portfolioname)
    <br />
    @Html.Label("Portfoliopic", new { @class = "form-control" })
    @Html.TextBoxFor(x => x.Portfoliopic)
    <br />
    <button class="btn btn-primary btn-user btn-block"> Güncelle</button>

</form>


